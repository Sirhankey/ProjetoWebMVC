        <button onclick="myFunction()">TesteButton</button>

        <script>
            function myFunction() {
                var y = document.getElementsByTagName("Comp")[0].childNodes[0].nodeValue;
                y.nodeValue = "Pedra";
            }
        </script>
    -->
</body>
</html>